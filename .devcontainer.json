{
    "name": "kafka-dev",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "service": "kafka-dev",
    "workspaceFolder": "/root/workspace",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "jeppeandersen.vscode-kafka",
                "ms-azuretools.vscode-docker",
                "eamodio.gitlens",
                "GitHub.copilot"
            ]
        }
    },
    "forwardPorts": [
        2181,
        9092,
        8080
    ]
}